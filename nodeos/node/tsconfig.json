{
  "compilerOptions": {
    "alwaysStrict": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitUseStrict": true,
    "module": "amd",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "sourceMap": false,
    "strict": true,
    "outDir": "../../dist",
    "target": "es5",
    "types": [
      "node"
    ],
    "lib": [
      "es6",
      "webworker"
    ],
    "newLine": "LF"
  },
  "exclude": [
    "dist",
    "../types"
  ]
}