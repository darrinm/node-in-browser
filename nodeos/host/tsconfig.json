{
  "compilerOptions": {
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "module": "es6",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "sourceMap": false,
    "strict": true,
    "target": "es6",
    "lib": [
      "es6",
      "dom"
    ],
    "newLine": "LF"
  }
}